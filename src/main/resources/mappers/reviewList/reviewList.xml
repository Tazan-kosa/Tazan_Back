<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ion.kosa.TazanBack.DAO.tourListDAO">
    <insert id="reviewUpload" parameterMap="ion.kosa.TazanBack.model.Review">
        select * from Review
    </insert>
    <select id="reviewDownload" resultType="ion.kosa.TazanBack.model.Review" parameterType="number">
        select * from Reivew where ReviewID = ${reviewID}
    </select>
</mapper>